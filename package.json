{
  "name": "colony-backend",
  "version": "0.0.1",
  "description": "Colony Backend apis",
  "author": "Danyal Rashid",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:migration:generate": "typeorm migration:generate -n",
    "typeorm:migration:run": "typeorm migration:run",
    "typeorm:migration:revert": "typeorm migration:revert",
    "typeorm:migration:create": "typeorm migration:create -n",
    "typeorm:migration:show": "typeorm migration:show",
    "typeorm:cache:clear": "typeorm cache:clear",
    "typeorm:model:generate": "typeorm-model-generator -h localhost -d te -u postgres -p 5432 -x dr903 -e postgres -o ./src/models -s public"
  },
  "dependencies": {
    "@casl/ability": "^4.1.6",
    "@nestjs/common": "^7.6.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.6.4",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.5",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.4",
    "@nestjs/platform-fastify": "^7.5.1",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.8",
    "@nestjs/typeorm": "^7.1.0",
    "@types/pica": "^5.1.2",
    "accepts": "^1.3.7",
    "apollo-server": "^2.19.1",
    "aws-sdk": "^2.817.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "change-case": "^4.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "common-tags": "^1.8.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "immer": "^7.0.14",
    "juice": "^7.0.0",
    "long": "^4.0.0",
    "lz-string": "^1.4.4",
    "marked": "^1.2.7",
    "moment": "^2.29.1",
    "nest-winston": "^1.4.0",
    "nodemailer": "^6.4.15",
    "papaparse": "^5.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.5.0",
    "pg": "^8.5.0",
    "pica": "^6.1.1",
    "pusher": "^3.0.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sift": "^13.5.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.25",
    "typeorm-model-generator": "^0.4.3",
    "typeorm-polymorphic": "0.0.10",
    "util": "^0.12.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "zone.js": "^0.10.3",
    "sqlite3": "5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.4",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.14.16",
    "@types/papaparse": "^5.0.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
